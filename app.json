{
  "expo": {
    "name": "sandbox-managed-app",
    "slug": "sandbox-managed-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.hiroapp.sandboxmanagedapp",
      "buildNumber": "1",
      "jsEngine": "hermes",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to output camera image."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.hiroapp.sandboxmanagedapp",
      "versionCode": 1,
      "jsEngine": "hermes",
      "permissions": ["CAMERA"]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
